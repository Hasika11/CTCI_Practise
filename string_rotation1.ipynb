    "substrings=[]\n",
    "def isrotation(s1,s2):\n",
    "    s1s1=s1+s1\n",
    "    for i in range(0,(len(s1s1)-len(s2))):\n",
    "                  substrings.append(s1s1[i:i+len(s2)])\n",
    "    if s2 in substrings:\n",
    "        return(True)\n",
    "    else:\n",
    "        return(False)\n",
    "isrotation('waterbottle','erbottlewat')"
   ]
